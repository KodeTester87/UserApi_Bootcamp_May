					"  type: \"object\",\r",
					"  required: [\"addressId\", \"plotNumber\", \"street\", \"state\", \"country\", \"zipCode\"],\r",
					"  properties: {\r",
					"    addressId: { type: \"integer\" },\r",
					"    plotNumber: { type: [\"string\", \"null\"] },\r",
					"    street: { type: \"string\" },\r",
					"    state: { type: \"string\" },\r",
					"    country: { type: \"string\" },\r",
					"    zipCode: { type: \"integer\" }\r",
					"  }\r",
					"};\r",
					"\r",
					"const userSchema = {\r",
					"  type: \"object\",\r",
					"  required: [\"userFirstName\", \"userLastName\", \"userContactNumber\", \"userEmailId\"],\r",
					"  properties: {\r",
					"    userId: { type: \"integer\" },\r",
					"    userFirstName: {\r",
					"      type: \"string\",\r",
					"      pattern: \"^[a-zA-Z][a-zA-Z ]+$\"\r",
					"    },\r",
					"    userLastName: {\r",
					"      type: \"string\",\r",
					"      pattern: \"^[a-zA-Z][a-zA-Z ]+$\"\r",
					"    },\r",
					"    userContactNumber: { type: \"integer\" },\r",
					"    userEmailId: {\r",
					"      type: \"string\",\r",
					"      pattern: \"^[a-zA-Z0-9]+(?:[._-][a-zA-Z0-9]+)*@[a-zA-Z0-9]+\\\\.[a-zA-Z]{2,}$\"\r",
					"    },\r",
					"    creationTime: { type: \"string\", format: \"date-time\" },\r",
					"    lastModTime: { type: \"string\", format: \"date-time\" },\r",
					"    userAddress: addressSchema\r",
					"  }\r",
					"};\r",
					"\r",
					"pm.collectionVariables.set(\"userSchema\", JSON.stringify(userSchema));\r",
					""
				]
