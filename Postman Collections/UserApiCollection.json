						{
							"name": "get All Users",
							"event": [
								{
									"listen": "test",
									"script": {
										"id": "a61ed661-fdff-4598-9616-6cba4e36868d",
										"exec": [
											"const Ajv = require('ajv');\r",
											"const ajv = new Ajv({ allErrors: true });\r",
											"\r",
											"const schema = JSON.parse(pm.collectionVariables.get(\"userSchema\"));\r",
											"const validate = ajv.compile(schema);\r",
											"\r",
											"const response = pm.response.json();\r",
											"\r",
											"pm.test(\"✅ Validate each user in array\", function () {\r",
											"  pm.expect(Array.isArray(response)).to.be.true;\r",
											"\r",
											"  response.forEach((user, index) => {\r",
											"    const valid = validate(user);\r",
											"    if (!valid) {\r",
											"      console.log(`❌ Validation failed at index ${index}:`, validate.errors);\r",
											"    }\r",
											"    pm.expect(valid, `User at index ${index} is invalid`).to.be.true;\r",
											"  });\r",
											"});\r",
											"pm.test(\"Status code is 200\", function () {\r",
											"    pm.response.to.have.status(200);\r",
											"    pm.response.to.be.json;\r",
											"});\r",
											"pm.test(\"Content-Type is present\", function () {\r",
											"    pm.response.to.have.header(\"Content-Type\");\r",
											"});"
										],
										"type": "text/javascript",
										"packages": {}
									}
								}
